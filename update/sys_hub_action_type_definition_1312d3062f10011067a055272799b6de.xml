        <value>(function execute(inputs, outputs) {
  outputs['arrinc'] = [];
  var arr = [];
  var text = "";

  var gr = new GlideRecord('incident');
  //gr.addQuery('subcategory', 'email');
  //gr.orderByDesc('sys_created_on');
  gr.query();
  if(gr.getRowCount() &gt; 0){
    while(gr.next()){
      if(gr.subcategory != null &amp;&amp; gr.subcategory != ''){
        gs.info("AA==" + gr.subcategory);
        if(gr.subcategory == "email"){
          arr.push(gr.number);
          text += gr.number + ",";
        }
      }
    }
  }

  outputs['text'] = text;
  outputs['arrinc'] = text.toString().split(",");
})(inputs, outputs);
</value>
